n         v-model=\"timeValue\"\r\n         type=\"daterange\"\r\n         start-placeholder=\"开始日期\"\r\n         end-placeholder=\"结束日期\"\r\n         :default-time=\"['00:00:00', '23:59:59']\">\r\n     </el-date-picker>\r\n     <el-button id=\"spider\" type=\"primary\" @click=\"onSpider\">爬取案件</el-button>\r\n   </div>\r\n\r\n<!--   <span>点击上传：</span><input type=\"file\" id=\"files1\" @change=\"uploadFile1()\">-->\r\n\r\n   <fieldset class=\"text-intro\">\r\n     <legend>案件信息</legend>\r\n     <legend>键入或选择爬取的案件txt文件</legend>\r\n   </fieldset>\r\n<!--   案件文件上传表单-->\r\n   <div class=\"test\">\r\n        <el-form style=\"width: 75%\" ref=\"form\"  id=\"caseText\" label-width=\"80px\">\r\n          <el-form-item style=\"margin-left: -18%;\" >\r\n            <el-input\r\n                style=\"width: 100%;\r\n                margin-top:20px\"\r\n                :rows=\"9\"\r\n                type=\"textarea\"\r\n                v-model=\"textValue\">{{textValue}}\r\n            </el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n   </div>\r\n\r\n   <div class=\"button-group\">\r\n     <el-upload action=\"https://jsonplaceholder.typicode.com/posts/\"\r\n                class=\"upload-demo\"\r\n                :auto-upload=\"false\"\r\n                :on-change=\"openFile\">\r\n       <el-button style=\"margin-left: 20px\" size=\"small\" type=\"primary\">上传案例文件</el-button>\r\n     </el-upload>\r\n     <el-button class=\"splitword-button\" size=\"small\" type=\"primary\" @click=\"splitText\">点击开始分词</el-button>\r\n   </div>\r\n\r\n<!--   <el-form ref=\"form\"  id=\"caseText\" label-width=\"80px\">-->\r\n<!--     <el-form-item label=\"案件信息\">-->\r\n<!--       <el-input v-model=\"textValue\" id=\"caseTextBox\">{{textValue}}</el-input>-->\r\n<!--     </el-form-item>-->\r\n<!--   </el-form>-->\r\n\r\n<!--   -->\r\n   <fieldset class=\"text-intro2\">\r\n     <legend>分词结果</legend>\r\n     <legend>点击可选项，对信息进行标注</legend>\r\n   </fieldset>\r\n\r\n<!--   分类进行标注-->\r\n   <el-tabs class=\"tag\" type=\"border-card\">\r\n     <el-tab-pane label=\"当事人\">\r\n       <div style=\"text-align: left\" v-for=\"(item, word, index) of tag\" :key=\"index\">\r\n<!--            span里是词性标题-->\r\n         <span class=\"tagTitle\" v-if=\"word === 'noun'\">名词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'verb'\">动词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'adjectives'\">形容词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'adverb'\">副词</span>\r\n<!--            下面是复选框-->\r\n            <el-checkbox-group style=\"margin-bottom: 12px\" v-model=\"selectedWords\">\r\n              <el-checkbox class=\"tagContent\" v-for=\"word in item\" :label=\"word\" :key=\"word\">\r\n                {{word}}\r\n              </el-checkbox>\r\n            </el-checkbox-group>\r\n         <div style=\"background:linear-gradient(to left,#F0F0F0,#b6b6b6,#F0F0F0);\r\n    height:1px;\r\n    position: relative;\r\n    margin-bottom: 9px\"></div>\r\n       </div>\r\n       <el-button style=\"margin-top: 10px\" size=\"small\" type=\"primary\" @click=\"saveAndSendMark('criminal')\">保存案件与标注</el-button>\r\n     </el-tab-pane>\r\n     <el-tab-pane label=\"性别\">\r\n       <div style=\"text-align: left\" v-for=\"(item, word, index) of tag\" :key=\"index\">\r\n         <!--            span里是词性标题-->\r\n         <span class=\"tagTitle\" v-if=\"word === 'noun'\">名词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'verb'\">动词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'adjectives'\">形容词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'adverb'\">副词</span>\r\n         <!--            下面是复选框-->\r\n         <el-checkbox-group style=\"margin-bottom: 10px\" v-model=\"selectedWords\">\r\n           <el-checkbox class=\"tagContent\" v-for=\"word in item\" :label=\"word\" :key=\"word\">\r\n             {{word}}\r\n           </el-checkbox>\r\n         </el-checkbox-group>\r\n         <div style=\"background:linear-gradient(to left,#F0F0F0,#b6b6b6,#F0F0F0);\r\n    height:1px;\r\n    position: relative;\r\n    margin-bottom: 10px\"></div>\r\n       </div>\r\n       <el-button style=\"margin-top: 10px\" size=\"small\" type=\"primary\" @click=\"saveAndSendMark('sex')\">保存案件与标注</el-button>\r\n     </el-tab-pane>\r\n     <el-tab-pane label=\"民族\">\r\n       <div style=\"text-align: left\" v-for=\"(item, word, index) of tag\" :key=\"index\">\r\n         <!--            span里是词性标题-->\r\n         <span class=\"tagTitle\" v-if=\"word === 'noun'\">名词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'verb'\">动词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'adjectives'\">形容词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'adverb'\">副词</span>\r\n         <!--            下面是复选框-->\r\n         <el-checkbox-group style=\"margin-bottom: 10px\" v-model=\"selectedWords\">\r\n           <el-checkbox class=\"tagContent\" v-for=\"word in item\" :label=\"word\" :key=\"word\">\r\n             {{word}}\r\n           </el-checkbox>\r\n         </el-checkbox-group>\r\n         <div style=\"background:linear-gradient(to left,#F0F0F0,#b6b6b6,#F0F0F0);\r\n    height:1px;\r\n    position: relative;\r\n    margin-bottom: 10px\"></div>\r\n       </div>\r\n       <el-button style=\"margin-top: 10px\" size=\"small\" type=\"primary\" @click=\"saveAndSendMark('ethic')\">保存案件与标注</el-button>\r\n     </el-tab-pane>\r\n     <el-tab-pane label=\"出生地\">\r\n       <div style=\"text-align: left\" v-for=\"(item, word, index) of tag\" :key=\"index\">\r\n         <!--            span里是词性标题-->\r\n         <span class=\"tagTitle\" v-if=\"word === 'noun'\">名词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'verb'\">动词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'adjectives'\">形容词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'adverb'\">副词</span>\r\n         <!--            下面是复选框-->\r\n         <el-checkbox-group style=\"margin-bottom: 10px\" v-model=\"selectedWords\">\r\n           <el-checkbox class=\"tagContent\" v-for=\"word in item\" :label=\"word\" :key=\"word\">\r\n             {{word}}\r\n           </el-checkbox>\r\n         </el-checkbox-group>\r\n         <div style=\"background:linear-gradient(to left,#F0F0F0,#b6b6b6,#F0F0F0);\r\n    height:1px;\r\n    position: relative;\r\n    margin-bottom: 10px\"></div>\r\n       </div>\r\n       <el-button style=\"margin-top: 10px\" size=\"small\" type=\"primary\" @click=\"saveAndSendMark('birth')\">保存案件与标注</el-button>\r\n     </el-tab-pane>\r\n     <el-tab-pane label=\"案由\">\r\n       <div style=\"text-align: left\" v-for=\"(item, word, index) of tag\" :key=\"index\">\r\n         <!--            span里是词性标题-->\r\n         <span class=\"tagTitle\" v-if=\"word === 'noun'\">名词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'verb'\">动词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'adjectives'\">形容词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'adverb'\">副词</span>\r\n         <!--            下面是复选框-->\r\n         <el-checkbox-group style=\"margin-bottom: 10px\" v-model=\"selectedWords\">\r\n           <el-checkbox class=\"tagContent\" v-for=\"word in item\" :label=\"word\" :key=\"word\">\r\n             {{word}}\r\n           </el-checkbox>\r\n         </el-checkbox-group>\r\n         <div style=\"background:linear-gradient(to left,#F0F0F0,#b6b6b6,#F0F0F0);\r\n    height:1px;\r\n    position: relative;\r\n    margin-bottom: 10px\"></div>\r\n       </div>\r\n       <el-button style=\"margin-top: 10px\" size=\"small\" type=\"primary\" @click=\"saveAndSendMark('reason')\">保存案件与标注</el-button>\r\n     </el-tab-pane>\r\n     <el-tab-pane label=\"相关法院\">\r\n       <div style=\"text-align: left\" v-for=\"(item, word, index) of tag\" :key=\"index\">\r\n         <!--            span里是词性标题-->\r\n         <span class=\"tagTitle\" v-if=\"word === 'noun'\">名词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'verb'\">动词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'adjectives'\">形容词</span>\r\n         <span class=\"tagTitle\" v-if=\"word === 'adverb'\">副词</span>\r\n         <!--            下面是复选框-->\r\n         <el-checkbox-group style=\"margin-bottom: 10px\" v-model=\"selectedWords\">\r\n           <el-checkbox class=\"tagContent\" v-for=\"word in item\" :label=\"word\" :key=\"word\">\r\n             {{word}}\r\n           </el-checkbox>\r\n         </el-checkbox-group>\r\n         <div style=\"background:linear-gradient(to left,#F0F0F0,#b6b6b6,#F0F0F0);\r\n    height:1px;\r\n    position: relative;\r\n    margin-bottom: 10px\"></div>\r\n       </div>\r\n       <el-button style=\"margin-top: 10px\" size=\"small\" type=\"primary\" @click=\"saveAndSendMark('lawHall')\">保存案件与标注</el-button>\r\n     </el-tab-pane>\r\n   </el-tabs>\r\n\r\n   <P id=\"tip\">\r\n     文本信息来源：裁判文书网\r\n   </P>\r\n   <P>\r\n     {{test}}\r\n   </P>\r\n\r\n   <el-button  type=\"success\" icon=\"el-icon-check\" circle @click=\"paintedEggShell\"></el-button>\r\n   <br>\r\n\r\n </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: 'example',\r\n  data(){\r\n    return{\r\n      test:'BAD TRAP',\r\n      timeValue: '',\r\n      textValue: '',\r\n      //category代表分词的词性\r\n      //words是分词结果\r\n      tag:{\r\n        // noun:['test10','test12','test13','test14','test15','test16','test17'],\r\n        // verb:['test20','test21','test22','test23','test24','test25','test26','test27'],\r\n        // adjectives:['test30','test31','test32','test33','test34','test35','test36','test37'],\r\n        // adverb:['test40','test41','test42','test43','test44','test45','test46','test47']\r\n        noun:[],\r\n        verb:[],\r\n        adjectives:[],\r\n        adverb:[]\r\n      },\r\n      //selectedWords存储分词的结果\r\n      selectedWords:[]\r\n    }\r\n  },\r\n  methods: {\r\n    onSpider: function (){\r\n      var startTime = this.timeValue[0].toLocaleDateString()\r\n      var endTime = this.timeValue[1].toLocaleDateString()\r\n      console.log(startTime, endTime)\r\n      console.log('后端处理一下字符串格式，把斜杠改为-')\r\n      this.$alert('已按照标注日期自动爬取案件！爬取文件保存于本地', '爬取案件', {\r\n        confirmButtonText: '确定',\r\n        callback: action => {\r\n          this.$message({\r\n            type: 'info',\r\n            message: `action: ${ action }`\r\n          });\r\n        }\r\n      });\r\n    },\r\n    openFile: function(file) {\r\n      var reader = new FileReader();\r\n      var that = this\r\n      reader.onload = function () {\r\n        if (reader.result) {\r\n          //打印文件内容\r\n          that.textValue = reader.result\r\n          console.log(that.textValue)\r\n        }\r\n      };\r\n      reader.readAsText(file.raw);\r\n    },\r\n    splitText: function (){\r\n      console.log('把txt文本传给后端')\r\n      //Alert\r\n      this.$alert('已成功进行分词！', '案例分词', {\r\n        confirmButtonText: '确定',\r\n      });\r\n    },\r\n    saveAndSendMark: function (tag){\r\n      //如果标注的是当事人\r\n      if(tag === 'criminal'){\r\n        this.$alert('已成功保存标注！', '当事人', {\r\n          confirmButtonText: '确定',\r\n        });\r\n      }\r\n      //如果标注的是性别\r\n      else if(tag === 'sex'){\r\n        this.$alert('已成功保存标注！', '性别', {\r\n          confirmButtonText: '确定',\r\n        });\r\n      }\r\n      //如果标注的是民族\r\n      else if(tag === 'ethic'){\r\n        this.$alert('已成功保存标注！', '民族', {\r\n          confirmButtonText: '确定',\r\n        });\r\n      }\r\n      //如果标注的是出生地\r\n      else if(tag === 'birth'){\r\n        this.$alert('已成功保存标注！', '出生地', {\r\n          confirmButtonText: '确定',\r\n        });\r\n      }\r\n      //如果标注的是案由\r\n      else if(tag === 'reason'){\r\n        this.$alert('已成功保存标注！', '案由', {\r\n          confirmButtonText: '确定',\r\n        });\r\n      }\r\n      //如果标注的是相关法院\r\n      else if(tag === 'lawHall'){\r\n        this.$alert('已成功保存标注！', '相关法院', {\r\n          confirmButtonText: '确定',\r\n          // callback: action => {\r\n          //   this.$message({\r\n          //     type: 'info',\r\n          //     message: `action: ${ action }`\r\n          //   });\r\n          // }\r\n        });\r\n      }\r\n      console.log(this.selectedWords)\r\n    },\r\n    paintedEggShell: function (){\r\n      const path = 'http://localhost:5000/data-science';\r\n      axios.get(path)\r\n          .then((res) => {\r\n            this.test = res.data;\r\n          })\r\n          .catch((error) => {\r\n            // eslint-disable-next-line\r\n            console.error(error);\r\n          });\r\n      // this.$alert('如果觉得本标注分析系统还不错的话就给个高分吧！👍', '感谢使用', {\r\n      //   confirmButtonText: '确定',\r\n      // });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n  .spider-intro{\r\n    margin-top: 20px;\r\n    border: 0;\r\n    font-size: large;\r\n    font-family: 方正苏新诗柳楷简体;\r\n    font-weight: bold;\r\n  }\r\n\r\n  .block{\r\n    margin-top: 0px;\r\n    position: relative;\r\n    height: 60px;\r\n    width: 44%;\r\n    margin-left: 28%;\r\n    /*border-radius: 30px;*/\r\n    /*border:1px solid #2c3e50*/\r\n  }\r\n\r\n  .text-intro {\r\n    margin-top: 20px;\r\n    border: 0;\r\n    border-top: 2px solid #ddd;\r\n    text-align: center;\r\n  }\r\n  legend {\r\n    font-family: 方正苏新诗柳楷简体;\r\n    font-size: large;\r\n    font-weight: bold;\r\n    padding: 0 5%;\r\n  }\r\n  .button-group{\r\n    position: relative;\r\n  }\r\n\r\n  #spider{\r\n    margin-left: 20px;\r\n  }\r\n\r\n  .splitword-button{\r\n    margin-top: 20px;\r\n    margin-left: 12px;\r\n    position: absolute;\r\n  }\r\n\r\n  .test{\r\n    align-content: center;\r\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\r\n    height : 240px;\r\n    width: 50%;\r\n    margin-left: 25%;\r\n  }\r\n\r\n  .upload-demo{\r\n    margin-left: 40%;\r\n    margin-top: 20px;\r\n    position: absolute;\r\n  }\r\n\r\n  #caseText{\r\n    width: 60%;\r\n    margin-left: 14%;\r\n    margin-top: 20px;\r\n\r\n  }\r\n\r\n  .text-intro2 {\r\n    margin-top: 110px;\r\n    border: 0;\r\n    border-top: 2px solid #ddd;\r\n    text-align: center;\r\n  }\r\n\r\n  .tag{\r\n    margin-top: 20px;\r\n    width: 50%;\r\n    margin-left: 25%;\r\n  }\r\n\r\n  .tagTitle{\r\n    font-family: \"思源宋体 CN Heavy\";\r\n    font-size: large;\r\n    font-weight: normal;\r\n  }\r\n\r\n  .tagContent{\r\n    font-family: \"Adobe Caslon Pro Bold\";\r\n    font-size: large;\r\n    margin-top: 10px;\r\n  }\r\n\r\n  #tip{\r\n    margin-top: 30px;\r\n    font-family: 方正苏新诗柳楷简体;\r\n    font-weight: bold;\r\n    font-size: large;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/components"}]}