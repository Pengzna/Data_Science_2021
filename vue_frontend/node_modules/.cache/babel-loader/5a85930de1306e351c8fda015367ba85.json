>\r\n     <div style=\"position: absolute\">数据科学大作业</div>\r\n   </div>\r\n\r\n\r\n   <div style=\"margin-top: 20px;\" class=\"block\">\r\n<!--     <p>组件值：{{ timeValue }}</p>-->\r\n     <el-date-picker\r\n         v-model=\"timeValue\"\r\n         type=\"daterange\"\r\n         start-placeholder=\"开始日期\"\r\n         end-placeholder=\"结束日期\"\r\n         :default-time=\"['00:00:00', '23:59:59']\">\r\n     </el-date-picker>\r\n     <el-button id=\"spider\" type=\"primary\" @click=\"onSpider\">爬取案件</el-button>\r\n   </div>\r\n\r\n<!--   <span>点击上传：</span><input type=\"file\" id=\"files1\" @change=\"uploadFile1()\">-->\r\n\r\n   <el-upload action=\"https://jsonplaceholder.typicode.com/posts/\"\r\n              class=\"upload-demo\"\r\n              :auto-upload=\"false\"\r\n              :on-change=\"openFile\">\r\n     <el-button size=\"small\" type=\"primary\">上传案例文件</el-button>\r\n   </el-upload>\r\n   <div class=\"test\">\r\n        <el-form style=\"width: 75%\" ref=\"form\"  id=\"caseText\" label-width=\"80px\">\r\n          <el-form-item style=\"margin-left: -10%\" label=\"案件信息\">\r\n            <el-input\r\n                style=\"width: 100%;\"\r\n                :rows=\"9\"\r\n                type=\"textarea\"\r\n                v-model=\"textValue\">{{textValue}}\r\n            </el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n   </div>\r\n   <el-button style=\"margin-top: 20px\" size=\"small\" type=\"primary\" @click=\"splitText\">点击开始分词</el-button>\r\n<!--   <el-form ref=\"form\"  id=\"caseText\" label-width=\"80px\">-->\r\n<!--     <el-form-item label=\"案件信息\">-->\r\n<!--       <el-input v-model=\"textValue\" id=\"caseTextBox\">{{textValue}}</el-input>-->\r\n<!--     </el-form-item>-->\r\n<!--   </el-form>-->\r\n\r\n\r\n   <P>\r\n     {{title}}\r\n   </P>\r\n\r\n   <el-button type=\"success\" icon=\"el-icon-check\" circle></el-button>\r\n   <br>\r\n\r\n </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'example',\r\n  data(){\r\n    return{\r\n      msg: 'Hello World!',\r\n      title: '裁判文书网',\r\n      timeValue: '',\r\n      textValue: ''\r\n    }\r\n  },\r\n  methods: {\r\n    onSpider: function (){\r\n      var startTime = this.timeValue[0].toLocaleDateString()\r\n      var endTime = this.timeValue[1].toLocaleDateString()\r\n      console.log(startTime, endTime)\r\n      console.log('后端处理一下字符串格式，把斜杠改为-')\r\n      console.log('开始爬取案件...')\r\n    },\r\n    openFile: function(file) {\r\n      var reader = new FileReader();\r\n      var that = this\r\n      reader.onload = function () {\r\n        if (reader.result) {\r\n          //打印文件内容\r\n          that.textValue = reader.result\r\n          console.log(that.textValue)\r\n        }\r\n      };\r\n      reader.readAsText(file.raw);\r\n    },\r\n    splitText: function (){\r\n      console.log('把txt文本传给后端')\r\n    }\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .test{\r\n    align-content: center;\r\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\r\n    height : 200px;\r\n    width: 50%;\r\n    margin-left: 25%;\r\n  }\r\n\r\n  #spider{\r\n    margin-left: 20px;\r\n  }\r\n\r\n  .upload-demo{\r\n    margin-top: 20px;\r\n  }\r\n\r\n  #caseText{\r\n    width: 60%;\r\n    margin-left: 20%;\r\n    margin-top: 20px;\r\n\r\n  }\r\n\r\n  /*#caseTextBox{*/\r\n  /*  height: 200px;*/\r\n  /*}*/\r\n</style>\r\n"],"sourceRoot":"src/components"}]}